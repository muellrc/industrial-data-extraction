create stream data_demo_stream  (plctag varchar, value INTEGER) 
with (kafka_topic = 'data_demo', value_format='JSON');

CREATE table plc_data with (value_format='JSON') AS 
select plctag
, value
from data_demo_stream WINDOW TUMBLING (size 5 minute);


create stream plc_stream with (value_format='JSON') as
select plctag
, value
from data_demo_stream ;


